<h1>Index of Student Resumes</h1>



<% @students.each do |student| %>
  <p>Photo: <img src="<%= student["photo"] %>"></p>
  <p><a href="<%=student["id"]%>">Name: <%= student["last_name"] %>, <%= student["first_name"] %></a></p>
  <p>Email: <%= student["email"] %></p>
  <p>Linkedin: <%= student["linkedin_url"] %></p>
  <p>Twitter: <%= student["twitter_handle"] %></p>
  <p>Resume website: <%= student["online_resume_url"] %></p>
  <p>Github: <%= student["github_url"] %></p>
    <hr>
<% end %>

